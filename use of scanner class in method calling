import java.util.*;

public class SimpleMethod {

	static void myMethod(String name, int no) {

		System.out.println("my name is " + " " + name + " " + "and college code is" + " " + no);
	}

	static int myMethod(int uniRollNo, int schoolRollNo) {
		return uniRollNo + schoolRollNo;
	}

	public static void main(String[] args) {
		
		while(true){
		Scanner obj = new Scanner(System.in);
		System.out.println("enter you name");
		System.out.println("enter your college code");
		String username = obj.nextLine();
		int code = obj.nextInt();
		System.out.println("usename is" + username);
		System.out.println("you college code is " + code);

		myMethod(username, code);
		System.out.println(19078 + 12); //for example.

	}}

}
